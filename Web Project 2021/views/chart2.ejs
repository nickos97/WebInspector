<script type = "text/javascript">
    function GetSelectedTextValue(select,select1) {
        
        ctypes = []
        ctypes1 = []

        var options = select && select.options;
        
        var opt;
        for(var i=0; i<options.length; i++) {
            opt = options[i];
            if(opt.selected) {
                ctypes.push(opt.text);
            }
        }
        var options1 = select1 && select1.options;
        
        var opt1;
        for(var i=0; i<options1.length; i++) {
            opt1 = options1[i];
            if(opt1.selected) {
                ctypes1.push(opt1.text);
            }
        }

        
        stypes=JSON.stringify(ctypes)
        stypes1=JSON.stringify(ctypes1)
        
        
        $.post('/chart2', {data: stypes, data1: stypes1}
        );
        setTimeout(()=>{
            location.href = "/chart2";
        
          })
        
        
    }
</script>
    <script type ="text/javascript">
        var ttls = "<%=ttls%>"
    
    
    
    
    ttls = ttls.replace(/&#34;/g,'"')
    ttls = JSON.parse(ttls)
    console.log(ttls)
    count = []
    
    for(var i in ttls) {
        count.push(ttls[i].count)
    }
    console.log(count)
    max_age =[]
    for(var j in ttls) {
        max_age.push(ttls[j].max_age)
    }
    console.log(max_age)
    max = Math.max(...max_age)
    min = Math.min(...max_age)
    
    bucket = (max - min)/10
    
    var dynamicColors = function() {
        var r = Math.floor(Math.random() * 255);
        var g = Math.floor(Math.random() * 255);
        var b = Math.floor(Math.random() * 255);
         
        return "rgb(" + r + "," + g + "," + b + ")";
     };
     color = []
     mydata = []
     for(var i=0; i<=9; i++) {
         mydata[i] = 0 
        color.push(dynamicColors());
     }
     ranges = []
     ranges[0] = {start:min, end:min + bucket}
     for(var i=1; i<=9; i++) {

        ranges[i] = {start: ranges[i-1].end, end: ranges[i-1].end + bucket}
     }
     ranges1=[]
     for(var i in ranges) {
         ranges1[i] = ranges[i].start + "-" + ranges[i].end
     }
     console.log(ranges)
     
    for(var i in ranges){
        for(var j in ttls) {
            if(parseInt(ttls[j].max_age)>=ranges[i].start && parseInt(ttls[j].max_age)<=ranges[i].end)
            mydata[i] = mydata[i] + ttls[j].count
        }
    }
    console.log(mydata)
     
   
     
     
     

     
    </script>
