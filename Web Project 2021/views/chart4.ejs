</head>
<body>
    <div class="nav-form">
        <nav>
          <h4 style=color:white>Web Inspector</h4>
          <ul>
              <li><a href="/Owner">Home</a></li>
              <li><a href="/updateacc">Account</a></li>
              <li><a>About us</a></li>
              <li><a href="/chart2" >Back</a></li>
              
          </ul>
        </nav>
      </div>

      <div class ="col-md-6 offset-md-3 ">
        <div class= "card">
           <div class="card-body">
          
               <h1>Chart 2</h1>
               <p style="color:red" class="card-text">Select multiple values by pressing Ctrl</p>
               <select multiple size = "7" id= "content_type">
                   <%for (var i in contypes) {%>
                       <option value = <%i%> ><%=contypes[i]%></option>
                   <%}%>
               </select>
               <select multiple size = "7" id= "isp">
                   <%for (var i in disp) {%>
                       <option value = <%i%> ><%=disp[i]%></option>
                   <%}%>
               </select>
               <button value = "select mutiple options" type="submit" onclick="var el = document.getElementsByTagName('select')[0];
               var el1 = document.getElementsByTagName('select')[1];
               (GetSelectedTextValue(el,el1));">Submit</button>
           </div>
           <div class = "card-body">
               <canvas id="myChart2" ></canvas>
           </div>
        </div>
       </div>
   
</body>
<script type = "text/javascript">
    function GetSelectedTextValue(select,select1) {
        
        ctypes = []
        ctypes1 = []

        var options = select && select.options;
        
        var opt;
        for(var i=0; i<options.length; i++) {
            opt = options[i];
            if(opt.selected) {
                ctypes.push(opt.text);
            }
        }
        var options1 = select1 && select1.options;
        
        var opt1;
        for(var i=0; i<options1.length; i++) {
            opt1 = options1[i];
            if(opt1.selected) {
                ctypes1.push(opt1.text);
            }
        }

        
        stypes=JSON.stringify(ctypes)
        stypes1=JSON.stringify(ctypes1)
        
        
        $.post('/chart4', {data: stypes, data1: stypes1}
        );
        setTimeout(()=>{
            location.href = "/chart4";
        
          })
        
        
    }
</script>
<script>
    var percs = "<%=prc%>"
    percs = percs.replace(/&#34;/g,'"')
    percs = JSON.parse(percs)
    cache=[]
    cache[0]=percs[0].public
    cache[1]=percs[0].private 
    cache[2]=percs[0].no_cache
    cache[3]=percs[0].no_store
    
   
    var dynamicColors = function() {
        var r = Math.floor(Math.random() * 255);
        var g = Math.floor(Math.random() * 255);
        var b = Math.floor(Math.random() * 255);
         
        return "rgb(" + r + "," + g + "," + b + ")";
     };
     color = []

     for(var i=0; i<=9; i++) {
        color.push(dynamicColors());
     }
</script>
<script>
    var ctx = 'myChart2';
mydata=['public','private','no-cache','no-store']
var myChart2 = new Chart(ctx, {
type: 'bar',
data: {
    labels: mydata,
    datasets: [{
        label: '% Cachability Percentage',
        data: cache,
        barThickness: 60,
        backgroundColor: color,
        borderColor: color,
        borderWidth: 1,
        
    }]
},
options: {
    
        xAxes: [{
            scaleLabel: {
              barPercentage:0.4,
              display: true,
              labelString: 'Hours'
            }
          }],
        yAxes: [{
            scaleLabel: {
              beginAtZero: true,
              display: true,
              labelString: 'Average time wait'
            }
          }]
    
}

});
</script>
</html>