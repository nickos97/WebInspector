    <title>Web Inspector</title>

    <style>
        .nav-form {
            background-color: #4d4d4d;
            color:cornsilk;
            padding: 30px 60px;
            display: flex;
            justify-content: space-between;
        }
        .nav-form h4 {
        text-align: center;
        color: #4d4d4d;
        font-size: 30px;
        padding: 20px 0 20px 0;
        }
        .nav-form ul {
            display:flex;
            justify-content:center;
            align-items:center;
        }
        .nav-form li {
            list-style: none;
            float:left;
            padding:10px;
        }
        .nav-form li a {
            color:cornsilk;
            text-decoration:none;
            font-weight:bold;
            padding:5px 8px;
            }
        .nav-form li a:hover {
            color: yellow; 
            
        }
        .lead {
            color:rgb(0, 0, 0);
            font-weight:bold;
        }
        .info-form {
            width: 300px;
            font-family: Tahoma, Geneva, sans-serif;
            text-align:center;
            
            }
        .info-form h2 {
            text-align: center;
            color: #4d4d4d;
            font-size: 25px;
            padding: 20px 0 20px 0;
            }
        .info-form button {
            color:rgb(31, 29, 28);
            width: 100px;
        }
        h3 {
            text-align: center;
            margin-top: -30px;
        }
